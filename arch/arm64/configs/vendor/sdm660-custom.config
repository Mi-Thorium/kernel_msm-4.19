# Android P System-as-root workarounds
CONFIG_INITRAMFS_IGNORE_SKIP_FLAG=y
CONFIG_PROC_CMDLINE_APPEND_ANDROID_FORCE_NORMAL_BOOT=y

# CPU
CONFIG_CPU_FREQ_STAT=y

# Fingerprint
CONFIG_FPR_FPC=n

# ION
CONFIG_ION_LEGACY=y

# KernelSU
CONFIG_KSU_STATIC_HOOKS=y

# Netfilter
CONFIG_IP_NF_TARGET_TTL=y

# OF
CONFIG_BUILD_ARM64_APPENDED_DTB_IMAGE=y

# Panic
CONFIG_PANIC_TIMEOUT=1

# Pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_LZ4_COMPRESS=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Ramdisk
CONFIG_RD_LZMA=y
CONFIG_RD_XZ=y

# Reduce size
CONFIG_SLUB_DEBUG=n

# Sched
CONFIG_SCHED_WALT=n
CONFIG_SCHED_CASS=y

# TCP Congestion Control
CONFIG_TCP_CONG_ADVANCED=y
CONFIG_TCP_CONG_BIC=y
CONFIG_TCP_CONG_WESTWOOD=y
CONFIG_TCP_CONG_HTCP=y
CONFIG_TCP_CONG_BBR=y
CONFIG_DEFAULT_BBR=y

# Userdata casefolding
CONFIG_UNICODE=y
CONFIG_SDCARD_FS=n

# Userdata casefolding
CONFIG_UNICODE=y
CONFIG_SDCARD_FS=n

# vDSO
CONFIG_COMPAT_VDSO=y

# WLAN
CONFIG_QCA_CLD_WLAN=y

# zRAM (LZ4)
CONFIG_ZRAM_DEFAULT_COMP_ALGORITHM="lz4"
CONFIG_CRYPTO_LZ4=y
CONFIG_LZ4_COMPRESS=y
CONFIG_LZ4_DECOMPRESS=y
