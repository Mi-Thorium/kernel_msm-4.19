# Localversion
CONFIG_LOCALVERSION_AUTO=y

# Fingerprint
CONFIG_FPR_FPC=n

# Mach
CONFIG_MACH_XIAOMI_MSM8937=y

# OF
CONFIG_OF_IGNORE_QCOM_DTS=y

# Panic
CONFIG_PANIC_TIMEOUT=1

# Pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_LZ4_COMPRESS=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Ramdisk
CONFIG_RD_LZMA=y

# Touchscreen
CONFIG_TOUCHSCREEN_SYSCTL_MI8937=y

# vDSO
CONFIG_COMPAT_VDSO=y

# WLAN
CONFIG_PRONTO_WLAN=y

# zRAM (LZ4)
CONFIG_ZRAM_DEFAULT_COMP_ALGORITHM="lz4"
CONFIG_CRYPTO_LZ4=y
CONFIG_LZ4_COMPRESS=y
CONFIG_LZ4_DECOMPRESS=y
