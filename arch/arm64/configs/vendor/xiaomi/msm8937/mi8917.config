# Localversion
CONFIG_LOCALVERSION="-Mi8937v2-Mi8917"

# Camera
CONFIG_MSMB_CAMERA_LEGACY=y

# IR
CONFIG_LEDS_GPIO_IR_MI8937=y
CONFIG_IR_PWM_MI8937=y
CONFIG_SPI_SPIDEV_IR_MI8937=y

# IR - PWM IR dependencies
CONFIG_MEDIA_RC_SUPPORT=y
CONFIG_RC_CORE=y
CONFIG_RC_MAP=y
CONFIG_RC_DECODERS=y
CONFIG_LIRC=y
CONFIG_IR_LIRC_CODEC=y
# CONFIG_USER_RC_INPUT is not set
# CONFIG_USER_SP_RC_INPUT is not set
CONFIG_IR_NEC_DECODER=y
CONFIG_IR_RC5_DECODER=y
CONFIG_IR_RC6_DECODER=y
CONFIG_IR_JVC_DECODER=y
CONFIG_IR_SONY_DECODER=y
CONFIG_IR_RC5_SZ_DECODER=y
CONFIG_IR_SANYO_DECODER=y
# CONFIG_IR_MCE_KBD_DECODER is not set
# CONFIG_IR_DUMP_DECODER is not set
CONFIG_RC_DEVICES=y
# CONFIG_RC_ATI_REMOTE is not set
# CONFIG_IR_IMON is not set
# CONFIG_IR_MCEUSB is not set
# CONFIG_IR_REDRAT3 is not set
# CONFIG_IR_STREAMZAP is not set
# CONFIG_IR_IGUANA is not set
# CONFIG_IR_TTUSBIR is not set
# CONFIG_RC_LOOPBACK is not set
# CONFIG_IR_GPIO_CIR is not set
# CONFIG_IR_GPIO is not set
# CONFIG_IR_MSM_GENI is not set

# LEDs
CONFIG_LEDS_AW2013_MI8937=y
CONFIG_LEDS_MSM_GPIO_FLASH=y

# Mach
CONFIG_MACH_XIAOMI_RIVA=y
CONFIG_MACH_XIAOMI_ROLEX=y
CONFIG_MACH_XIAOMI_UGGLITE=y

# Power supply
CONFIG_MSM_USB_POWER_SUPPLY_MI8937=y
CONFIG_CHARGER_BQ2560X_MI8937=y
CONFIG_FUEL_GAUGE_BQ27426_MI8937=y
CONFIG_BATTERY_CW2015_ROLEX=y
CONFIG_SMB358_CHARGER_ROLEX=y

# Regulator
CONFIG_REGULATOR_TPS65132_MI8937=y

# Touchscreen
CONFIG_TOUCHSCREEN_FTS_MI8937=y
CONFIG_TOUCHSCREEN_GT9XX_MI8937=y

# Vibrator
CONFIG_LEDS_VIBRATOR_GPIO_MI8937=y
